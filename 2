import java.util.Scanner;

public class hello {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int num = input.nextInt();

        System.out.println(num);

        if (num % 2 == 0){
            System.out.printf("even");
        }
        else {
            System.out.printf("odd");
        }
    }
}
